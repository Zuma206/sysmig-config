{
  "include": [
    "~/.config/waybar/workspaces.jsonc",
    "~/.config/waybar/battery.jsonc",
    "~/.config/waybar/pulseaudio.jsonc",
    "~/.config/waybar/layout.jsonc",
    "~/.config/waybar/wlogout.jsonc"
  ],
  "tray": {
    "spacing": 15
  },
  "memory": {
    "format": "󰍛 {used}"
  },
  "cpu": {
    "format": "󰻠 {usage}%"
  },
  "temperature": {
    "format": " {temperatureC}°C"
  }
}
